#!/usr/bin/env bash

cd ..

writeInFile=true;


./waf shell &

./build/scratch/ECE445_Lab2 --distance=1 --writeInFile=$writeInFile --useFrii=true
for distance in {10..1000..10}
do
	./build/scratch/ECE445_Lab2 --distance=$distance --writeInFile=$writeInFile --useFrii=true
done

./build/scratch/ECE445_Lab2 --distance=1 --writeInFile=$writeInFile --useFrii=false
for distance in {10..1000..10}
do
	./build/scratch/ECE445_Lab2 --distance=$distance --writeInFile=$writeInFile --useFrii=false
done



