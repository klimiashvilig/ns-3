#!/usr/bin/env bash

cd ..

writeInFile=true;
routing=true;
fileSize=200;

./waf shell &

for distance in {75..600..75}
do
	for runNum in {1..30}
	do
		if [ "$runNum" -ne 30 ]
		then
			./wifi-adhoc-var-distance --distance=$distance --runNum=$runNum --writeInFile=$writeInFile --routing=$routing --fileSize=$fileSize
		else
			./wifi-adhoc-var-distance --distance=$distance --runNum=$runNum --writeInFile=$writeInFile --routing=$routing --fileSize=$fileSize --endLine=true
		fi
	done
done



