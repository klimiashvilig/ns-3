#!/usr/bin/env bash

cd ..

writeInFile=true;
routing=true;
distance=75;

./waf shell &

for fileSize in {512..20480..512}
do
	for streamNumber in {1..10}
	do
		for runNum in {1..5}
		do
			if [ "$runNum" -ne 5 ] || [ "$streamNumber" -ne 10 ]
			then
				./build/scratch/wifi-adhoc-var-file-size-2-flows --distance=$distance --runNum=$runNum --writeInFile=$writeInFile --routing=$routing --fileSize=$fileSize --streamNumber=$streamNumber
			else
				./build/scratch/wifi-adhoc-var-file-size-2-flows --distance=$distance --runNum=$runNum --writeInFile=$writeInFile --routing=$routing --fileSize=$fileSize --endLine=true --streamNumber=$streamNumber
			fi
		done
	done
done



