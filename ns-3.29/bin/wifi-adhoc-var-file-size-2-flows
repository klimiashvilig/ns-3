#!/usr/bin/env bash

cd ..

writeInFile=true;
routing=true;
distance=75;

./waf shell &

for fileSize in {100..10000..100}
do
	for runNum in {1..30}
	do
		if [ "$runNum" -ne 30 ]
		then
			./build/scratch/wifi-adhoc-var-file-size-2-flows --distance=$distance --runNum=$runNum --writeInFile=$writeInFile --routing=$routing --fileSize=$fileSize
		else
			./build/scratch/wifi-adhoc-var-file-size-2-flows --distance=$distance --runNum=$runNum --writeInFile=$writeInFile --routing=$routing --fileSize=$fileSize --endLine=true
		fi
	done
done



